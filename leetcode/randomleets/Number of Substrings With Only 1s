class Solution:
    def numSub(self, s: str) -> int:
        
        res=0
        track=[0]
        for i in range(len(s)):
            if s[i]=='1':
                track.append(track[-1]+1)
            else:
                track.append(0)
        for j in track:
            res+=j
        return res %(10**9+7)
        
