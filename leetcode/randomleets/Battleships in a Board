class Solution:
    def countBattleships(self, board: List[List[str]]) -> int:
        res=0
        for i in range(len(board)):
            for j in range(len(board[0])):
                if board[i][j]=='X':
                    flag=1
                    if i>0 and board[i-1][j]!='.':flag=0
                    if j>0 and board[i][j-1]!='.':flag=0
                    res+=flag
        return res
