class Solution:
    def compareVersion(self, version1: str, version2: str) -> int:

        
        c1=[int(i) for i in version1.split('.')]
        c2=[int(i) for i in version2.split('.')]
        
        for i in range(max(len(c1),len(c2))):
            c=c1[i] if i<len(c1) else 0
            d=c2[i] if i<len(c2) else 0
            if c>d:return 1
            elif c<d:return -1
        return 0
       

