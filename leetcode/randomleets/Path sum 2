# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def pathSum(self, root: Optional[TreeNode], targetSum: int) -> List[List[int]]:
        res=[]
        def dfs(node,curr,lis):
            if node:
                if not node.left and not node.right:
                    curr+=node.val
                    if curr==targetSum:
                        lis.append(node.val)
                        res.append(lis)
                    
                else:
                    dfs(node.left,curr+node.val,lis+[node.val])
                    dfs(node.right,curr+node.val,lis+[node.val])
            return

        dfs(root,0,[])
        return res
        
