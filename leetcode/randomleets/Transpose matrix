class Solution:
    def transpose(self, matrix: List[List[int]]) -> List[List[int]]:
        mat=[[0 for i in range(len(matrix))] for j in range(len(matrix[0]))]
        for i in range(len(mat)):
            for j in range(len(mat[0])):         
                mat[i][j]=matrix[j][i]
        return mat
