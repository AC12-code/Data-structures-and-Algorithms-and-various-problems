class Solution:
    def numSquares(self, n: int) -> int:
        if n<2:return n
        lis=[]
        for i in range(1,n):
            if i*i<=n:
                lis.append(i*i)
        c={n}
        res=0
        while c:
            res+=1
            temp=set()
            for i in c:
                for j in lis:
                    if i==j:return res
                    if i<j:break
                
                    temp.add(i-j)
            c=temp
        return res
    
